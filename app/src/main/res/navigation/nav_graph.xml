<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navigation_attend_check">
    <fragment
        android:id="@+id/navigation_attend_check"
        android:name="com.example.smartattendancecheckapp.presentation.ui.attendcheck.AttendCheckFragment"
        android:label="AttendCheckFragment"
        tools:layout="@layout/fragment_attend_check">
        <action
            android:id="@+id/action_attent_check_to_enroll"
            app:destination="@id/navigation_enroll" />
    </fragment>
    <fragment
        android:id="@+id/navigation_calendar"
        android:name="com.example.smartattendancecheckapp.presentation.ui.CalendarCheckFragment"
        android:label="CalendarCheckFragment"
        tools:layout="@layout/fragment_calendar_check"/>
    <fragment
        android:id="@+id/navigation_seat_away"
        android:name="com.example.smartattendancecheckapp.ui.SeatAwayFragment"
        android:label="SeatAwayFragment"
        tools:layout="@layout/fragment_seat_away"/>

    <fragment
        android:id="@+id/navigation_enroll"
        android:name="com.example.smartattendancecheckapp.presentation.ui.enrollface.EnrollFaceFragment"
        android:label="EnrollFaceFragment/"
        tools:layout="@layout/fragment_enroll_face">
        <action
            android:id="@+id/action_navigation_enroll_to_navigation_attend_check"
            app:destination="@id/navigation_attend_check" />
    </fragment>
</navigation>